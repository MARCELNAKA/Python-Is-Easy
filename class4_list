#option 1

myUniqueList = []
result = False

print (myUniqueList)

newItem = "a"

if newItem not in myUniqueList:
    myUniqueList.append (newItem)
    result = True
else:
    result = False

print (myUniqueList)
print (result)

#option 2
myUniqueList = []

result = False
newItem = "a"

if  newItem in (myUniqueList):
    result = False
else:
    result = True
    myUniqueList.append (newItem)

print (result)
print (newItem)

#extra

myUniqueList = []
myLeftovers = []

newItem = "a"

if newItem not in myUniqueList:
    myUniqueList.append (newItem)
    else:
    myLeftovers.append (newItem)

print (myUniqueList)
print (myLeftovers)
